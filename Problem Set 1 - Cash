#include <cs50.h>
#include <math.h>
#include <stdio.h>

int main(void)
{
    float change;
    do
    {
        change = get_float("Change owed: ");
    } while (change < 0);
    int cents = round(change * 100);

    int coins = 0;

    coins += cents / 25;
    cents %= 25;

    coins += cents / 10;
    cents %= 10;

    coins += cents / 5;
    cents %= 5;

    coins += cents / 1;
    cents %= 1;

    printf("%i", coins);
}
